#include <stdio.h>
int main(void) {
    int t, n;
    //Enter number of test cases
    scanf("%d",&t);
    for(int i = 0; i < t; i++) {
        //ENTER NUMBER OF TERMS FOR THE ARRAY
        scanf("%d",&n);
        int a[n];
        for(int j = 0; j < n; j++) {
            //ENTER ARRAY FOR THE TERM
            scanf("%d",&a[j]);
        }
        int s = 0;
        for(int k = 0; k < n; k++) {
            if(a[k] <= a[k+1]) {
                s++;
            }
        }
        if(s >= n - 1) {
            printf("Yes\n");
        }
        else {
            printf("No\n");
        }
    }
	return 0;
}
